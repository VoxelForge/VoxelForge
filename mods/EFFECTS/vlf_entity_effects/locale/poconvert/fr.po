msgid "Lingering @1@2 Potion"
msgstr "Potion @1@2 persistante"

msgid "Arrow of @1@2"
msgstr "Flêche de @1@2"

msgid "@1 + Potion"
msgstr "@1 + Potion"

msgid "Splash @1 + Potion"
msgstr "Potion @1 + jetable"

msgid "Lingering @1 + Potion"
